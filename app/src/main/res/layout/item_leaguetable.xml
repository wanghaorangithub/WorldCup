<?xml version="1.0" encoding="utf-8"?>


<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="343.7dp"
    android:layout_height="186.3dp"

    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp" />

    <RelativeLayout
        android:id="@+id/item_matchlist_relativelayout"
        android:layout_width="350dp"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary">

        <TextView
            android:id="@+id/leaguetable_item_textview1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="23dp"
            android:layout_marginTop="9.3dp"
            android:lineSpacingExtra="7.3sp"
            android:textColor="#000000"
            android:textSize="22.3sp"
            tools:text="A组" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="23dp"
            android:layout_marginTop="52dp"
            android:orientation="vertical">


            <TextView
                android:id="@+id/leaguetable_item_textview2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:lineSpacingExtra="7.3sp"
                android:textColor="#000000"
                android:textSize="22.3sp"
                tools:text="1. 阿根廷：4分" />

            <TextView
                android:id="@+id/leaguetable_item_textview3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:lineSpacingExtra="7.3sp"
                android:textColor="#000000"
                android:textSize="22.3sp"
                tools:text="2. 阿根廷：4分" />

            <TextView
                android:id="@+id/leaguetable_item_textview4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:lineSpacingExtra="7.3sp"
                android:textColor="#000000"
                android:textSize="22.3sp"
                tools:text="3. 阿根廷：4分" />

            <TextView
                android:id="@+id/leaguetable_item_textview5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:lineSpacingExtra="7.3sp"
                android:textColor="#000000"
                android:textSize="22.3sp"
                tools:text="4. 阿根廷：4分" />
        </LinearLayout>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="5dp" />
</LinearLayout>